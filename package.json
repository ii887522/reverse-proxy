{
  "name": "@ii887522/reverse-proxy",
  "version": "2.0.0",
  "description": "It is a server that forwards client request to the correct web server for processing, and also forwards server response to the correct web client browser for presentation.",
  "main": "index.js",
  "bin": "index.js",
  "type": "module",
  "scripts": {
    "lint": "ts-standard",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ii887522/reverse-proxy.git"
  },
  "keywords": [
    "server",
    "proxy",
    "proxy-server",
    "reverse-proxy"
  ],
  "author": "ii887522",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ii887522/reverse-proxy/issues"
  },
  "homepage": "https://github.com/ii887522/reverse-proxy#readme",
  "ts-standard": {
    "ignore": ["**/*.js"]
  },
  "dependencies": {
    "@ii887522/hydro": "^2.0.1",
    "express": "^4.17.2",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/http-proxy": "^1.17.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "cross-env": "^7.0.3",
    "jest": "^27.4.7",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.5"
  }
}
